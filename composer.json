{
  "name": "yuncms/yii2-composer",
  "description": "The composer plugin for yuncms module installer",
  "keywords": ["yii2", "composer", "yuncms", "module installer"],
  "type": "composer-plugin",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/yuncms/yii2-composer/issues",
    "source": "https://github.com/yuncms/yii2-composer"
  },
  "authors": [
    {
      "name": "XUTL",
      "email": "xutl@gmail.com"
    }
  ],
  "require": {
    "composer-plugin-api": "^1.0"
  },
  "require-dev": {
    "composer/composer": "^1.0"
  },
  "autoload": {
    "psr-4": { "yuncms\\composer\\": "" }
  },
  "extra": {
    "class": "yuncms\\composer\\Plugin",
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
